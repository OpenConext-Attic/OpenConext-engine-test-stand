{
    "name": "openconext/engineblock-tester",
    "description": "Surfnet OpenConext SAML proxy",
    "require": {
        "simplesamlphp/simplesamlphp": "dev-trunk",
        "simplesamlphp/saml2": "0.2.0",
        "openid/php-openid": "dev-master#a287b2d85e753c84b3b883ed8ee3ffe8692c8477 as 2.2.2",
        "silex/silex": "~1.1"
    },
    "require-dev": {
        "behat/behat": "2.4.*",
        "behat/mink": "1.5.0",
        "behat/mink-extension": "1.1.4",
        "behat/mink-goutte-driver": "1.0.9"
    },
    "autoload": {
        "psr-0": { "OpenConext\\EngineTestStand": "src/" }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "robrichards/xmlseclibs",
                "version": "1.3.1",
                "source": {
                    "type": "svn",
                    "url": "http://xmlseclibs.googlecode.com/svn",
                    "reference": "trunk@50"
                },
                "autoload": {
                    "files": ["xmlseclibs.php"]
                }
            }
        }
    ]
}
